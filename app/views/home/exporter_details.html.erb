<h3 class="page-title">
Exporter Details			
</h3>
<ul class="breadcrumb">
<li>
	<i class="icon-home"></i>
	<a href="index.html">Home</a> 
	<i class="icon-angle-right"></i>
</li>
<li><a href="#">Exporter Details</a></li>
</ul>

<% unless @exporters.empty? %>

	<div class = "widget-body">
	  <table class="table table-striped table-bordered table-advance table-hover">
	    <thead>
	      <tr>
	        <th>Name</th>
	        <th>Address</th>
	        <th>City</th>
	        <th>Zip Code</th>
	        <th>Email</th>
	        <th>Contact Person</th>
	        <th>Phone</th>
	        <th>Created At</th>
	        <th> Edit </th>
	      </tr>
	    </thead>
	    <tbody>
	      <% @exporters.each do |exporter| %>
	        <tr>
	        	<td><%= exporter.name %></td>
	        	<td><%= exporter.address_1 %></td>
	        	<td><%= exporter.city %></td>
	        	<td><%= exporter.zip_code %></td>
	        	<td><%= exporter.email %></td>
	        	<td><%= exporter.contact_person %></td>
	        	<td><%= exporter.phone %></td>
	          <td><%= exporter.created_at.strftime('%Y-%m-%d') rescue "-" %></td>
	          <td><a class="btn btn-mini" href="#"><i class="icon-edit"></i> Edit</a></td>
	        </tr>
	      <% end %>     
	    </tbody>
	  </table>
	</div>

<% else %>
 No data Found
<% end -%>